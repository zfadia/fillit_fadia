#include <unistd.h>
int main(void);

void ft_putchar(char c)
{
	write (1, &c, 1);
}
void ft_print_numbers()
{
	int i;

	i = '0';
	while (i <= '9')
	{
		ft_putchar();
		i++;
	}		
	return();
}
